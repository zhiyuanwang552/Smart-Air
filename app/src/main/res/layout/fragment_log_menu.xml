<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/button9"
        android:layout_width="150dp"
        android:layout_height="357dp"
        android:layout_marginEnd="44dp"
        android:backgroundTint="#03A9F4"
        android:text="Controller\nlog"
        android:textAllCaps="false"
        android:textSize="22sp"
        app:cornerRadius="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/rescue_button"
        app:strokeColor="#03A9F4" />

    <Button
        android:id="@+id/rescue_button"
        android:layout_width="150dp"
        android:layout_height="359dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="132dp"
        android:backgroundTint="#03A9F4"
        android:text="Rescuer log"
        android:textSize="22sp"
        app:cornerRadius="0dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#2196F3"
        app:strokeWidth="2dp" />

    <Button
        android:id="@+id/button10"
        android:layout_width="0dp"
        android:layout_height="74dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="44dp"
        android:backgroundTint="#2196F3"
        android:text="@string/add_medical_log_title"
        android:textSize="20sp"
        app:cornerRadius="0dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/rescue_button" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="292dp"
        android:layout_height="88dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.426"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logo_icon" />
</androidx.constraintlayout.widget.ConstraintLayout>